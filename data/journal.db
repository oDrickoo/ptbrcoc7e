{"folder":"JfhzDvm55N35SZmx","name":"031","pages":[{"sort":100000,"name":"Picada de Aranha","type":"image","_id":"31pn9DUAgqL9HDLk","title":{"show":true,"level":1},"image":{"caption":""},"text":{"format":1},"video":{"controls":true,"volume":0.5},"src":"worlds/ptbrcoc7e/baralhos/eventos%20desastrosos/031.png","system":{},"ownership":{"default":-1},"flags":{}}],"sort":0,"ownership":{"default":0,"or4CzIOBXaFQWJlw":3},"flags":{},"_stats":{"systemId":"CoC7","systemVersion":"0.9.2","coreVersion":"10.287","createdTime":1667763115979,"modifiedTime":1667763130508,"lastModifiedBy":"or4CzIOBXaFQWJlw"},"_id":"XfjcVWSXc6pLVaXn"}

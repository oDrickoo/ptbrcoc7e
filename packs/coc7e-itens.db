mas me\u0002dievais similares.</p>\n<p>Motosserras (10%): a primeira motosserra alimentada por gasolina produzida em massa surgiu em 1927, por&eacute;m exis\u0002tiam vers&otilde;es anteriores</p>\n<p>&nbsp;</p>","opposingDifficulty":"","pushedFaillureConsequences":"","chat":"","keeper":""},"base":"25","adjustments":{"personal":null,"occupation":null,"archetype":null,"experience":null},"value":null,"attributes":{},"properties":{"special":true,"rarity":false,"push":true,"combat":true,"fighting":true,"firearm":false},"eras":{},"flags":{},"skillName":"Brigar"}},{"_id":"jQIwhbwERPFu6Cs9","name":"Todas as Perícias (Natação)","type":"skill","img":"systems/CoC7/assets/icons/skills/swim.svg","data":{"specialization":"Todas as Perícias","description":{"value":"","opposingDifficulty":"","pushedFaillureConsequences":"","chat":"","keeper":""},"base":"20","adjustments":{"personal":null,"occupation":null,"archetype":null,"experience":null},"value":null,"attributes":{},"properties":{"special":true,"rarity":false,"push":true,"combat":false},"eras":{},"flags":{},"skillName":"Natação"},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"9kX3HnNjpoyg9O9p":3},"flags":{},"system":{"specialization":"Todas as Perícias","description":{"value":"","opposingDifficulty":"","pushedFaillureConsequences":"","chat":"","keeper":""},"base":"20","adjustments":{"personal":null,"occupation":null,"archetype":null,"experience":null},"value":null,"attributes":{},"properties":{"special":true,"rarity":false,"push":true,"combat":false},"eras":{},"flags":{},"skillName":"Natação"}}],"attributes":{},"properties":{},"eras":{},"flags":{}},"_stats":{"systemId":"CoC7","systemVersion":"0.9.2","coreVersion":"10.287","createdTime":1664995267844,"modifiedTime":1666035712786,"lastModifiedBy":"or4CzIOBXaFQWJlw"},"_id":"o76A9VOgyw39dlHp","folder":null,"sort":0,"ownership":{"default":0,"or4CzIOBXaFQWJlw":3}}
